{
  "version": 2,
  "name": "videogen-admin-panel",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BACKEND_URL": "@backend_url"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_BACKEND_URL": "@backend_url"
    }
  }
}